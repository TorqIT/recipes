{
    "manifests": {
        "torq/pimcore-frontend": {
            "manifest": {
                "env": {
                    "FRONTEND_PORT_EXTERNAL": 3400
                },
                "docker-compose": {
                    "docker-compose.yaml": {
                        "services": [
                            "  frontend:",
                            "    container_name: ${APP_NAME}-frontend",
                            "    image: node:latest",
                            "    working_dir: /frontend",
                            "    volumes:",
                            "    - ./frontend:/frontend:rw",
                            "    command: bash -c \"cd frontend; npm ci; npm run dev;\"",
                            "    ports:",
                            "    - \"${FRONTEND_PORT_EXTERNAL}:8080\""
                        ]
                    }
                },
                "post-install-output": [
                    "* Run <bg=blue;fg=white;options=bold>npm create vite@latest frontend -- --template react-ts</> to initialize a Vite frontend."
                ]
            },
            "files": [],
            "ref": "6ff0b06f820f51d76d7cec66ff888a8be9fba39b"
        }
    }
}
